{
    "name": "azurlane",
    "version": "1.0.0",
    "description": "Api wrapper for the azur lane api",
    "main": "lib/index",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "homepage": "https://kurozeropb.github.io/AzurLane/index.html",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KurozeroPB/AzurLane.git"
    },
    "keywords": [
        "azurlane",
        "azur",
        "lane",
        "api",
        "azur-lane",
        "shipgirls",
        "waifu"
    ],
    "bugs": {
        "email": "bugs@kurozeropb.info",
        "url": "https://github.com/KurozeroPB/AzurLane/issues"
    },
    "author": {
        "email": "root@kurozeropb.info",
        "name": "KurozeroPB",
        "url": "https://kurozeropb.info"
    },
    "license": "MIT",
    "private": false,
    "scripts": {
        "prepare": "yarn run rollup:build && yarn run types:generate",
        "rollup:build": "rollup -c && rm -rf ./lib/src",
        "rollup:watch": "yarn run rollup:build -- --watch",
        "types:generate": "dts-bundle-generator -o lib/index.d.ts src/index.ts",
        "test": "yarn jest && yarn tslint:test && yarn eslint:test",
        "jest": "jest --config jest.config.js --no-cache",
        "tslint:test": "tslint -p ./",
        "docs": "typedoc && cp .nojekyll docs/",
        "gts:check": "gts check",
        "gts:clean": "gts clean",
        "gts:fix": "gts fix",
        "eslint:test": "eslint --ext .ts .",
        "eslint:fix": "eslint --ext .ts --fix ."
    },
    "dependencies": {
        "axios": "^0.19.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.20",
        "@types/node": "^12.11.7",
        "@typescript-eslint/eslint-plugin": "^2.6.0",
        "@typescript-eslint/parser": "^2.6.0",
        "dts-bundle-generator": "^3.2.0",
        "eslint": "^6.6.0",
        "gh-pages": "^2.1.1",
        "gts": "^1.1.0",
        "jest": "^24.9.0",
        "rollup": "^1.26.0",
        "rollup-plugin-ignore": "^1.0.5",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-typescript": "^1.0.1",
        "rollup-plugin-typescript2": "^0.25.1",
        "ts-node": "^8.4.1",
        "tslib": "^1.10.0",
        "tslint": "^5.20.0",
        "typedoc": "^0.15.0",
        "typescript": "^3.6.4"
    }
}
